{
    "onmessage": {
        "linkinfo":  {
            "permit" : "*",
            "match"  : "https?:\\/\\/[www\\.]?.*?\\..*?(?=\\/|$)",
            "call"   : "linkinfo",
            "reply"  : {
                "action" : "{}"
            }
        }
    },
    "onjoin": {
        "fetchmemo": {
            "permit" : "*",
            "call"   : "fetchmemo",
            "reply"  : {
                "action" : "[me]: Messages have been left for you while you were offline, they have been PM'd to you.",
                "private" : "{}"
            }
        }
    },
    "onleave": {

    }
}
